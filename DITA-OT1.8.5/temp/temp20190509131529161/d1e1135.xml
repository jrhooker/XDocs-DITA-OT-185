    [-n|--namespace=&lt;nameSpace&gt;] [-p|--prpoffset=&lt;offset&gt;]
    [-a|--listoffset=&lt;offset&gt;] [-e|--listentryoffset=&lt;offset&gt;]
    [-v|--rsvd=&lt;setReserved&gt;] [-g|--psdt=&lt;prpSglDataXfer&gt;]
    [-o|--output=&lt;file&gt;] [-s|--seed]
    [-x|--expstat=&lt;StatusCodeType&lt;&lt;8|StatusCode&gt;]
    [-f|--fua=&lt;forceUnitAccess&gt;] </codeph></p>

