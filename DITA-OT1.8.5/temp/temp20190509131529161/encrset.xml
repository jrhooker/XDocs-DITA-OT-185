    [--keyTagL=&lt;keyTagL&gt;] [--k00=&lt;dataDEK00&gt;]
    [--k01=&lt;dataDEK01&gt;] [--k02=&lt;dataDEK02&gt;]
    [--k03=&lt;dataDEK03&gt;] [--k04=&lt;dataDEK04&gt;]
    [--k05=&lt;dataDEK05&gt;] [--k06=&lt;dataDEK06&gt;]
    [--k07=&lt;dataDEK07&gt;] [--k10=&lt;dataDEK10&gt;]
    [--k11=&lt;dataDEK11&gt;] [--k12=&lt;dataDEK12&gt;]
    [--k13=&lt;dataDEK13&gt;] [--k14=&lt;dataDEK14&gt;]
    [--k15=&lt;dataDEK15&gt;] [--k16=&lt;dataDEK16&gt;]
    [--k17=&lt;dataDEK17&gt;] [-x --expstat=&lt;int&gt;</codeph>] </p>

