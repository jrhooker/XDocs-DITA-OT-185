    <table>
      <?guidance Use this table to define the series of registers to be included in a header file. ?>

      <title/>

      <tgroup cols="4">
        <colspec colname="A" colwidth="14*"/>

        <colspec colname="B" colwidth="14*"/>

        <colspec colname="C" colwidth="23*"/>

        <colspec colname="D" colwidth="49*"/>

        <thead>
          <row>
            <entry><p>Address</p></entry>

            <entry><p>Size</p></entry>

