    <note audience="MSCCInternal" class="- topic/note " xtrf="C:\Git\XDocs-DITA-OT-185\XDocs-DITA-OT-185\DITA-OT1.8.5\samples\digi\Other\Internal_Flow_Control_Buffer_uiu8j35d3.xml" xtrc="note:1;11:35"><xref href="http://bby1dms01.pmc-sierra.internal/DocMgmt/getfile.cfm?file_id=427198" scope="external" class="- topic/xref " xtrf="C:\Git\XDocs-DITA-OT-185\XDocs-DITA-OT-185\DITA-OT1.8.5\samples\digi\Other\Internal_Flow_Control_Buffer_uiu8j35d3.xml" xtrc="xref:2;11:137"><?ditaot usertext?>
    http://bby1dms01.pmc-sierra.internal/DocMgmt/fileinfo.cfm?file_id=427198
    </xref> Hysteresis Buffer tab</note>

    <fig id="Hysteresis_buffer_contributions_tiu8j35d3" class="- topic/fig " xtrf="C:\Git\XDocs-DITA-OT-185\XDocs-DITA-OT-185\DITA-OT1.8.5\samples\digi\Other\Internal_Flow_Control_Buffer_uiu8j35d3.xml" xtrc="fig:1;15:57">
