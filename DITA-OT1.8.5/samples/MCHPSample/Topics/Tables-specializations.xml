            <entry><p>Details</p></entry>
          </row>
        </thead>

        <reg-references xmlns:deltaxml="http://www.deltaxml.com/ns/well-formed-delta-v1">
          <reg-reference>
            <reg-address>0x2000</reg-address>

            <reg-size>4</reg-size>

            <reg-name>Name</reg-name>

            <reg-details>
              <xref href="Tables-specializations.xml#xd_c23f8a4d95f95c5f--66ce64d0-15d85b8a809--7e5a/table_Message_Dword_Table_-_MSG-DESCRIPTION_1j5mybk8z">Message
