                  <entry colname="col1" class="- topic/entry " xtrf="C:\Git\XDocs-DITA-OT-185\XDocs-DITA-OT-185\DITA-OT1.8.5\samples\diffed\_b-0-file-\Trident\device\devspec\ntb\trident_ntb_ntb_example_tlp_walkthrough.xml" xtrc="entry:9;228:41">
