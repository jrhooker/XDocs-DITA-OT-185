    -b|--nlba=&lt;numLBA&gt; [-m|--nbuf=&lt;numberOfbuffers&gt;]
    [-n|--namespace=&lt;nameSpace&gt;] [-p|--offset=&lt;offset&gt;]
    [-a|--listoffset=&lt;offset&gt;] [-e|--listentryoffset=&lt;offset&gt;]
    [-i|--input=&lt;file&gt;] [-s|--seed] [-d|--data=&lt;data&gt;]
